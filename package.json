        "graphql": "^15.4.0",
        "react-query": "^3.5.16",
        "react-redux": "^7.2.4",
        "@reduxjs/toolkit": "^1.6.0",
        "next-i18next": "^7.0.1",
        "prettier": "^2.2.1"
    },
    "devDependencies": {
        "@types/node": "^14.14.13",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
